{
	"extends": [
		"eslint-config-hudochenkov",
		"eslint-config-prettier"
	],
	"env": {
		"browser": false,
		"node": true,
		"es6": true,
		"jest": true
	},
	"globals": {
		"groupTest": true,
		"runTest": true
	},
	"rules": {
		"prefer-destructuring": ["error"],
		"prefer-rest-params": ["error"],
		"prefer-spread": ["error"]
	}
}
